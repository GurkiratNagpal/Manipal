package com.telstra;
import java.util.*;
public class Newborn {
	private String name;
	private  int months;
	private double weight;
	public Hospital h;
	public Newborn(String name, int months, double weight, int patientID) {
		super();
		this.name = name;
		this.months = months;
		this.weight = weight;
		h = new Hospital(patientID, name);
	}

	
	
}
