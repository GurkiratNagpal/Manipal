package com.telstra;
import java.util.*;

public class Largest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc= new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		
		int large1 = (a>b)?((a>c)?a:c):((b>c)?b:c);
		System.out.print("largest number: "+ large1 );

	}

}
